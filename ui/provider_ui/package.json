{
  "name": "parallex-provider-dashboard",
  "version": "0.0.1",
  "author": "Parallex",
  "description": "Parallex Provider Dashboard",
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.0",
    "@mui/styled-engine": "5.12.3",
    "@mui/x-date-pickers": "^6.19.4",
    "@reduxjs/toolkit": "^2.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "axios": "^1.6.8",
    "chart.js": "3.9.1",
    "chroma-js": "2.4.2",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "os-browserify": "^0.3.0",
    "prop-types": "15.8.1",
    "react": ">=16.8.0",
    "react-alert": "^7.0.3",
    "react-alert-template-basic": "^1.0.2",
    "react-alert-template-mui": "^1.0.7",
    "react-chartjs-2": "3.0.5",
    "react-countdown": "^2.3.5",
    "react-countup": "6.4.2",
    "react-dom": ">=16.8.0",
    "react-flatpickr": "3.10.13",
    "react-redux": "^9.1.0",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "stylis": "4.2.0",
    "stylis-plugin-rtl": "2.1.1",
    "uuid": "9.0.0",
    "web-vitals": "3.3.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "install:peer-deps": "npm install --legacy-peer-deps",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.2.2",
    "electron-builder": "^24.9.1",
    "electronmon": "^2.0.2",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.8.8",
    "react-app-rewired": "^2.2.1",
    "wait-on": "^7.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "overrides": {
    "svgo": "3.0.2"
  },
  "build": {
    "appId": "com.parallex.provider",
    "productName": "Parallex Provider Dashboard",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
